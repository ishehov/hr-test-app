{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "import/prefer-default-export": 0,
        "indent": ["warn", 4, { "SwitchCase": 1 }],
        "arrow-parens": ["warn", "as-needed"],
        "eol-last": "warn",
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "react/destructuring-assignment": 0,
        "react/jsx-indent": ["warn", 4],
        "react/jsx-indent-props": ["warn", 4],
        "max-len": ["warn", 120]
    }
}
